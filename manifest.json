{
	"manifest_version": 2,
	"name": "Notebooks Web Clipper",
	"description": "web clipper for notebooksapp",
	"version": "0.0.1",
	"icons": {
		"64": "icons/64.png",
      "256": "icons/256.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": {
			"64": "icons/64.png",
			"256": "icons/256.png"
		},
		"default_title": "Sendx to Notebooks"
	},
	"permissions": ["tabs"]
}
